Parameter A B C : Prop.

Lemma ex1_1 : A -> B -> A.
Proof.
  intros.
  assumption.
Qed.


Lemma ex1_2 : (A -> B -> C) -> (A -> B) -> A -> C.
Proof.
  intros.
  apply H.
  assumption.
  apply H0.
  assumption.
Qed.

Lemma ex1_3 : A /\ B -> B.
Proof.
  intros.
  elim H.
  destruct H.
Qed.

Lemma ex1_4 : B -> A \/ B.
Proof.

Qed.

Lemma ex1_5 : (A \/ B) -> (A -> C) -> (B -> C) -> C.
Proof.

Qed.

Lemma ex1_6 : A -> bot -> !A.
Proof.

Qed.
